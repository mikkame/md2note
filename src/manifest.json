{
  "name": "Chrome Extension Webpack",
  "content_scripts":[
    {
      "matches":[
        "https://note.com/notes/*"
      ],
      "js":[
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "icons/icon-128.png"
  },
  "manifest_version": 2,
  "version": "1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
